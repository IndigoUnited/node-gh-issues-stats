{
  "name": "gh-issues-stats",
  "version": "1.0.0",
  "description": "Collect statistical information about issues of a GitHub repository",
  "main": "index.js",
  "scripts": {
    "lint": "eslint '{*.js,lib/**/*.js,test/**/*.js}' --ignore-pattern=test/coverage",
    "test": "NOCK_BACK_MODE=record mocha --bail -t 20000",
    "test-cov": "NOCK_BACK_MODE=record istanbul cover --dir test/coverage _mocha -- --bail -t 20000 && echo Coverage lies in test/coverage/lcov-report/index.html",
    "test-travis": "NOCK_BACK_MODE=lockdown istanbul cover _mocha --report lcovonly -- --bail && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "bugs": {
    "url": "https://github.com/IndigoUnited/node-gh-issues-stats/issues/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/IndigoUnited/node-gh-issues-stats.git"
  },
  "keywords": [
    "gh",
    "github",
    "issues",
    "stats",
    "statistics"
  ],
  "author": "IndigoUnited <hello@indigounited.com> (http://indigounited.com)",
  "license": "MIT",
  "dependencies": {
    "github-parse-link": "^1.1.1",
    "got": "^6.1.2",
    "lodash.merge": "^4.3.1",
    "p-throttler": "^1.0.2",
    "token-dealer": "^1.1.1"
  },
  "devDependencies": {
    "@satazor/eslint-config": "^2.0.0",
    "chai": "^3.4.1",
    "coveralls": "^2.11.6",
    "eslint": "^2.2.0",
    "istanbul": "^0.4.1",
    "mocha": "^2.3.4",
    "nock": "^7.5.0"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
